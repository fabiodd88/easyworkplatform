
<!-- DIV PER IL CLIENTE -->
<div id="articolo" style="display: none;">
	<div class="container div-box text-center">

		<div class="row-fluid">
			<div class="col-md-6">
				<div class="form-group">
					<label>Nome</label> <input type="text" class="form-control"
						name="name" readonly>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label>Descrizione</label> <input type="text" class="form-control"
						name="surename" readonly>
				</div>
			</div>
		</div>

		<div class="row-fluid">
			<div class="col-md-6">
				<div class="form-group">
					<label>Prezzo</label> <input type="text" name="date"
						class="form-control" name="taxCode" readonly>
				</div>
			</div>
			<div class="col-md-6">
				<div class="form-group">
					<label>Durata</label> <input type="text" class="form-control"
						name="time" readonly>
				</div>
			</div>
		</div>
		<div class="col-md-12">
			<div class="col-md-10"></div>
			<div class="col-md-2">
				<div class="col-md-3">
					<button class="but" data-toggle="modal"
						data-target="#mioModalArticle">
						<img src="icon/modifica.png"><br>Modifica
					</button>
				</div>
				<div class="col-md-1"></div>
				<div class="col-md-3">
					<button class="but">
						<img src="icon/remove.png"><br>Elimina
					</button>
				</div>
			</div>
		</div>
	</div>

</div>
